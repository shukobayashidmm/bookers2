<p id="notice"></p>
<div class="contaiter px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
      <%= render 'users/show', users:@users %>
      <%= render 'books/new', books:@books %>
    </div>
    <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="#"></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", "/users/#{user.id}" %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

