    <h2>Books</h2>
    <table class="table table-hover table-inverse">
     <thead>
      <tr>
       <th></th>
       <th>Title</th>
       <th>Opinion</th>
       <th colspan="3"></th>
      </tr>
     </thead>
     <tbody>
      <% books.each do |book| %>
      <tr>
       <td><%= link_to (image_tag @user.get_profile_image), books_path(@book_params) %></td>
       <td><%= link_to book.title, books_path(@book_params) %></td>
       <td></td>
      </tr>
      <% end %>
     </tbody>
    </table>